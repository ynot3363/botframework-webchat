{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/spfx-serve.schema.json",
  "port": 4321,
  "https": true,
  "serveConfigurations": {
    "default": {
      "pageUrl": "https://{tenantDomain}/sites/WebChat",
      "customActions": {
        "bd8c84d5-8721-4b94-8d48-98b57819fb15": {
          "location": "ClientSideExtension.ApplicationCustomizer",
          "properties": {
            "directLineToken": "<YOUR DIRECT LINK TOKEN>"
          }
        }
      }
    },
    "webChat": {
      "pageUrl": "https://{tenantDomain}.sharepoint.com",
      "customActions": {
        "bd8c84d5-8721-4b94-8d48-98b57819fb15": {
          "location": "ClientSideExtension.ApplicationCustomizer",
          "properties": {
            "directLineToken": "<YOUR DIRECT LINK TOKEN>"
          }
        }
      }
    }
  }
}
